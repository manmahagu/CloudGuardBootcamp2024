{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "adminaccess",
			"Effect": "Allow",
			"Action": "*",
			"Resource": "*",
			"Condition": {
				"StringEquals": {
					"aws:RequestedRegion": [
						"us-east-1",
						"us-east-2",
						"eu-central-1",
						"us-west-2",
						"eu-west-2",
						"ap-southeast-1",
						"ap-southeast-2"
					]
				}
			}
		}
	]
}
